{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      ".git*",
      "node_modules/**",
      ".firebase/**",
      "package.json",
      "package-lock.json"
    ],
    "rewrites": [
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
